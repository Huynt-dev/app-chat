er, history }));\n  };\n\n  const users = useSelector((state) => state.user.users);\n\n  const searchUser = (e) => {\n    const text = e.target.value;\n    const res = text.toLowerCase();\n    const find = users.filter((x) => x.name.toLowerCase().includes(res));\n    // dispatch(setUsers(find));\n    users = find;\n  };\n\n  return (\n    <Container>\n      <div>\n        <Search\n          className=\"Search\"\n          placeholder=\"tìm kiếm...\"\n          onChange={searchUser}\n        />\n        <Row1>\n          {users.map((x, index) => {\n            return (\n              <Col1 key={index} span={12}>\n                <div>\n                  <BoxMessage>\n                    <BadgeA>\n                      <Badge status={x.isOnline ? \"success\" : \"default\"}>\n                        <AvatarA\n                          shape=\"square\"\n                          size={60}\n                          src={x.avatar}\n                          icon={<UserOutlined />}\n                        />\n                      </Badge>\n                    </BadgeA>\n\n                    <TextA>\n                      <h3>{x.name}</h3>\n                      <p>\n                        Email: <span>{x.email}</span>\n                      </p>\n                    </TextA>\n\n                    <Btn>\n                      <Button type=\"primary\">Add Friend</Button>\n                      <Button\n                        onClick={() => {\n                          findUser(x._id);\n                        }}\n                      >\n                        Send Messages\n                      </Button>\n                    </Btn>\n                  </BoxMessage>\n                </div>\n              </Col1>\n            );\n          })}\n        </Row1>\n      </div>\n    </Container>\n  );\n};\n\nexport default Users;\n",["214","215"],"/home/huynt/project-app-chat/app-chat/src/configs/index.js",[],"/home/huynt/project-app-chat/app-chat/src/components/Menu/noti